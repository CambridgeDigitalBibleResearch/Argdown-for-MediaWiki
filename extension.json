{
	"name": "Argdown",
	"author": [
		"James Harris"
	],
	"url": "https://github.com/DawnPaladin/Argdown",
	"descriptionmsg": "Parse Argdown on wiki pages",
	"license-name": "MIT",
	"type": "tag",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadClasses": {
		"Argdown": "hello_world.php"
	},
	"AutoloadNamespaces": {
	},
	"config": {},
	"Hooks": {
		"ParserFirstCallInit": "Argdown::onParserFirstCallInit"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Argdown/resources"
	},
	"ResourceModules": {
		"ext.Argdown": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"manifest_version": 2
}
